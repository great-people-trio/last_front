{
  "manifest_version": 3,
  "name": "Bookmark Manager",
  "version": "1.0",
  "description": "Save and view your bookmarks.",
  "permissions": ["storage", "activeTab"],
  "action": {
    "default_popup": "bookmark_check/bookmark_check.html",
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "icons": {
    "16": "icon16.png"
  }
}

